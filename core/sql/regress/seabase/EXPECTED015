CREATE TABLE TRAFODION.SCH.T015PRESP6
  (
    A                                INT NO DEFAULT NOT NULL NOT DROPPABLE
  , B                                TIMESTAMP(6) NO DEFAULT NOT NULL NOT
      DROPPABLE
  , C                                VARCHAR(20) CHARACTER SET ISO88591 COLLATE
      DEFAULT DEFAULT NULL
  , PRIMARY KEY (A ASC, B ASC)
  )
RANGE SPLIT BY ("_DIVISION_1_", A) (ADD FIRST KEY ( 201508, 1) , ADD FIRST KEY ( 201508, 100) , ADD FIRST KEY ( 201509, 1) )
  /* ACTUAL REGIONS 4 */
  DIVISION BY (DATE_PART('YEARMONTH',B)
     NAMED AS ("_DIVISION_1_"))
;

