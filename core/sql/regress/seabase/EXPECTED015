CREATE TABLE TRAFODION.SCH.T015PRESP5
  (
    SI                               SMALLINT NO DEFAULT NOT NULL NOT DROPPABLE
  , II                               INT NO DEFAULT NOT NULL NOT DROPPABLE
  , LI                               LARGEINT NO DEFAULT NOT NULL NOT DROPPABLE
  , N8                               NUMERIC(8, 2) NO DEFAULT NOT NULL NOT
      DROPPABLE
  , DT                               DATE NO DEFAULT NOT NULL NOT DROPPABLE
  , TM                               TIME(0) NO DEFAULT NOT NULL NOT DROPPABLE
  , TS                               TIMESTAMP(3) NO DEFAULT NOT NULL NOT
      DROPPABLE
  , CI                               CHAR(5) CHARACTER SET ISO88591 COLLATE
      DEFAULT NO DEFAULT NOT NULL NOT DROPPABLE
  , VC                               VARCHAR(7 BYTES) CHARACTER SET UTF8
      COLLATE DEFAULT NO DEFAULT NOT NULL NOT DROPPABLE
  , NC                               VARCHAR(10) CHARACTER SET UCS2 COLLATE
      DEFAULT NO DEFAULT NOT NULL NOT DROPPABLE
  , IYM                              INTERVAL YEAR(2) TO MONTH NO DEFAULT NOT
      NULL NOT DROPPABLE
  , IDF                              INTERVAL DAY(2) TO SECOND(6) NO DEFAULT
      NOT NULL NOT DROPPABLE
  , IHM                              INTERVAL HOUR(6) TO MINUTE NO DEFAULT NOT
      NULL NOT DROPPABLE
  , I8                               INTERVAL SECOND(12,6) NO DEFAULT NOT NULL
      NOT DROPPABLE
  , PRIMARY KEY (SI ASC, II ASC, LI ASC, N8 ASC, DT ASC, TM ASC, TS ASC, CI
    ASC, VC ASC, NC ASC, IYM ASC, IDF ASC, IHM ASC, I8 ASC)
  )
RANGE SPLIT BY (SI, II, LI, N8, DT, TM, TS, CI, VC, NC, IYM, IDF, IHM, I8) (ADD FIRST KEY ( 1, 1, 1, 1.11, DATE '2015-01-01', TIME '08:15:00', TIMESTAMP '2015-01-01 08:15:00.000000', _ISO88591'abcd', _UTF8'abcde', _UCS2'ABCDE', INTERVAL '1-11' YEAR(2) TO MONTH, INTERVAL '1 11:11:11.111111' DAY(2) TO SECOND(6), INTERVAL '111111:11' HOUR(6) TO MINUTE, INTERVAL '111111.111111' SECOND(12,6)) , ADD FIRST KEY ( 2, 2, 2, 1.11, DATE '2015-01-01', TIME '08:15:00', TIMESTAMP '2015-01-01 08:15:00.000000', _ISO88591'abcd', _UTF8'abcde', _UCS2'ABCDE', INTERVAL '1-11' YEAR(2) TO MONTH, INTERVAL '1 11:11:11.111111' DAY(2) TO SECOND(6), INTERVAL '111111:11' HOUR(6) TO MINUTE, INTERVAL '111111.111111' SECOND(12,6)) )
  /* ACTUAL REGIONS 3 */
;

