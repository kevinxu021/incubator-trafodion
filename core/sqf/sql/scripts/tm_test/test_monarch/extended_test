sqlci -i ../delete_all.sql | tee delete.log

lv_start_time=`date`
sqlci -i gendata_1_1000.sql > gendata_1_1000.log &

sqlci -i gendata_1001_2000.sql > gendata_1001_2000.log &

echo "Launched two sqlci sessions in the background"
jobs

wait %2
lv_end_time=`date`
echo "End of the jobs"
jobs

sqlci -i ../validate.sql | tee validate.log

echo "Main Job Start time: ${lv_start_time}"
echo "Main Job End   time: ${lv_end_time}"
